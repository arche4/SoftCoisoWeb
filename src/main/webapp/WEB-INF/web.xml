<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>CargaArchivoServlet</display-name>
    <filter>
        <filter-name>FilterSecurity</filter-name>
        <filter-class>com.softcoisoweb.servlet.FilterSecurity</filter-class>
    </filter>
    <filter-mapping> 
        <filter-name>FilterSecurity</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalendarServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.CalendarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CargarDatosCalendarServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.CargarDatosCalendarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesionServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.CerrarSesionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonaServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.PersonaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CasoServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.CasoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExpedienteServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.ExpedienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CargaArchivoServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.CargaArchivoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.UsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicamentosServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.MedicamentosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TipoCasoServlet</servlet-name>
        <servlet-class>com.softcoisoweb.servlet.TipoCasoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CerrarSesionServlet</servlet-name>
        <url-pattern>/CerrarSesionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CalendarServlet</servlet-name>
        <url-pattern>/CalendarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CargarDatosCalendarServlet</servlet-name>
        <url-pattern>/CargarDatosCalendarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonaServlet</servlet-name>
        <url-pattern>/PersonaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CasoServlet</servlet-name>
        <url-pattern>/CasoServlet</url-pattern>
    </servlet-mapping>    
    <servlet-mapping>
        <servlet-name>ExpedienteServlet</servlet-name>
        <url-pattern>/ExpedienteServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>CargaArchivoServlet</servlet-name>
        <url-pattern>/CargaArchivoServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/UsuarioServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>MedicamentosServlet</servlet-name>
        <url-pattern>/MedicamentosServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>TipoCasoServlet</servlet-name>
        <url-pattern>/TipoCasoServlet</url-pattern>
    </servlet-mapping> 
    <session-config>
        <session-timeout>
            50
        </session-timeout>
    </session-config>
</web-app>
